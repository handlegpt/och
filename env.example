# Supabase 配置
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# 可选：Stripe 支付配置（用于订阅功能）
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# 认证配置
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_RESEND_API_KEY=your_resend_api_key

# Sentry错误监控配置
VITE_SENTRY_DSN=https://your_sentry_dsn_here@o4510095342567424.ingest.us.sentry.io/4510095349579776

# Google Analytics配置
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Stripe支付配置
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# 应用配置
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# 成本控制配置
VITE_COST_CONTROL_ENABLED=true
VITE_FREE_USER_DAILY_LIMIT=0.1
VITE_FREE_USER_MONTHLY_LIMIT=2.0

# 速率限制配置
VITE_RATE_LIMIT_ENABLED=true
VITE_RATE_LIMIT_REQUESTS_PER_MINUTE=10
VITE_RATE_LIMIT_REQUESTS_PER_HOUR=100

# 数据库配置
VITE_DB_POOL_SIZE=20
VITE_DB_CONNECTION_TIMEOUT=30000

# 监控配置
VITE_MONITORING_ENABLED=true
VITE_PERFORMANCE_MONITORING=true
VITE_ERROR_REPORTING=true

# 安全配置
VITE_SECURITY_HEADERS=true
VITE_CSP_ENABLED=true
VITE_HTTPS_ONLY=true

# 缓存配置
VITE_CACHE_TTL=3600
VITE_CACHE_MAX_SIZE=100

# 日志配置
VITE_LOG_LEVEL=info
VITE_LOG_TO_CONSOLE=true
VITE_LOG_TO_SENTRY=true
